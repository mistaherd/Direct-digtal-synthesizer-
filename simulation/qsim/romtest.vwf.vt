`timescale 1 ps/ 1 ps
module DSS_vlg_sample_tst(
	clkin,
	data_port_0,
	data_port_1,
	sampler_tx
);
input  clkin;
input [4:0] data_port_0;
input [4:0] data_port_1;
output sampler_tx;

reg sample;
time current_time;
always @(clkin or data_port_0 or data_port_1)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module DSS_vlg_check_tst (
	FSK_OUT,
	sampler_rx
