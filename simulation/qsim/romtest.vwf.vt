// generate trigger
always @(FSK_OUT_expected or FSK_OUT)
begin
	trigger <= ~trigger;
end

always @(posedge sampler_rx or negedge sampler_rx)
begin
`ifdef debug_tbench
	$display("Scanning pattern %d @time = %t",i,$realtime );
	i = i + 1;
	$display("| expected FSK_OUT = %b | ",FSK_OUT_expected_prev);
	$display("| real FSK_OUT = %b | ",FSK_OUT_prev);
`endif
	if (
		( FSK_OUT_expected_prev[0] !== 1'bx ) && ( FSK_OUT_prev[0] !== FSK_OUT_expected_prev[0] )
		&& ((FSK_OUT_expected_prev[0] !== last_FSK_OUT_exp[0]) ||
			on_first_change[1])
	)
	begin
		$display ("ERROR! Vector Mismatch for output port FSK_OUT[0] :: @time = %t",  $realtime);
		$display ("     Expected value = %b", FSK_OUT_expected_prev);
		$display ("     Real value = %b", FSK_OUT_prev);
		nummismatches = nummismatches + 1;
		on_first_change[1] = 1'b0;
