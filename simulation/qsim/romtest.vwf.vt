module DSS_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clkin;
reg [4:0] data_port_0;
reg [4:0] data_port_1;
// wires                                               
wire [7:0] ASK_OUT;
wire [4:0] LFSR_ASK;
wire [7:0] LUT_OUT;

wire sampler;                             

// assign statements (if any)                          
DSS i1 (
// port map - connection between master ports and signals/registers   
	.ASK_OUT(ASK_OUT),
	.clkin(clkin),
	.data_port_0(data_port_0),
	.data_port_1(data_port_1),
	.LFSR_ASK(LFSR_ASK),
	.LUT_OUT(LUT_OUT)
);

// clkin
always
begin
	clkin = 1'b0;
	clkin = #1000 1'b1;
	#1000;
end 

DSS_vlg_sample_tst tb_sample (
	.clkin(clkin),
	.data_port_0(data_port_0),
	.data_port_1(data_port_1),
	.sampler_tx(sampler)
);

DSS_vlg_check_tst tb_out(
	.ASK_OUT(ASK_OUT),
	.LFSR_ASK(LFSR_ASK),
	.LUT_OUT(LUT_OUT),
	.sampler_rx(sampler)
);
endmodule

