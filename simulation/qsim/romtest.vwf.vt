input sampler_rx;

reg [7:0] FSK_OUT_expected;

reg [7:0] FSK_OUT_prev;

reg [7:0] FSK_OUT_expected_prev;

reg [7:0] last_FSK_OUT_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:1] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 1'b1;
end

// update real /o prevs

always @(trigger)
begin
	FSK_OUT_prev = FSK_OUT;
end

// update expected /o prevs

always @(trigger)
begin
	FSK_OUT_expected_prev = FSK_OUT_expected;
end


// expected FSK_OUT[ 7 ]
initial
begin
	FSK_OUT_expected[7] = 1'bX;
end 
// expected FSK_OUT[ 6 ]
